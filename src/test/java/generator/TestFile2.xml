<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
	objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

	<changeSet author="phan" id="1" dbms="mssql">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="NDER_RELATED_ID" />
			</not>
		</preConditions>

		<comment>Create table NDER_RELATED_ID</comment>
		<createTable tableName="NDER_RELATED_ID">

			<column name="id_nder_related_id" type="integer" autoIncrement="true">
				<constraints primaryKeyName="P_NDER_RELATED_ID" primaryKey="true" />
			</column>

		</createTable>

	</changeSet>

	<changeSet author="phan" id="2" dbms="oracle, postgresql">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="NDER_RELATED_ID" />
			</not>
		</preConditions>

		<comment>Create table NDER_RELATED_ID</comment>
		<createTable tableName="NDER_RELATED_ID">

			<column name="id_nder_related_id" type="integer">
				<constraints primaryKeyName="P_NDER_RELATED_ID" primaryKey="true" />
			</column>

		</createTable>

	</changeSet>

	<changeSet author="phan" id="3" dbms="oracle">
		<preConditions onFail="MARK_RAN">
			<not>
				<sequenceExists sequenceName="SEQ_NDER_RELATED_ID" />
			</not>
		</preConditions>
		<comment>Create sequence SEQ_NDER_RELATED_ID on Oracle if it doesn't exist.</comment>
		<createSequence sequenceName="SEQ_NDER_RELATED_ID" startValue="50" incrementBy="50"
			cacheSize="20" maxValue="999999999999999999999999999" ordered="false" cycle="false" />
	</changeSet>

	<changeSet author="phan" id="4" dbms="postgresql">
		<preConditions onFail="MARK_RAN">
			<not>
				<sequenceExists sequenceName="SEQ_NDER_RELATED_ID" />
			</not>
		</preConditions>
		<comment>Create sequence SEQ_NDER_RELATED_ID on postgreSQL if it doesn't exist.</comment>
		<createSequence sequenceName="SEQ_NDER_RELATED_ID" startValue="50" incrementBy="50"
			cacheSize="20" maxValue="999999999999999999" cycle="false" />
	</changeSet>

	<changeSet author="phan" id="5">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="version_id" />
			</not>
		</preConditions>
		<comment>Add column version_id to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="version_id" type="integer">
				<constraints nullable="false" />
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="6">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="messageid" />
