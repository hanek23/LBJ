			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="relatedcontainer" />
			</not>
		</preConditions>
		<comment>Add column relatedcontainer to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="relatedcontainer" type="integer">
				<constraints foreignKeyName="F_NDER_REL_REL_CONTAINER"
					referencedTableName="NDER_CONTAINER" referencedColumnNames="id_nder_container" />
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="12">
		<preConditions onFail="MARK_RAN">
			<not>
				<indexExists indexName="I_NDER_RELATED_CONTAINER" />
			</not>
		</preConditions>
