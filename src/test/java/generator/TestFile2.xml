	<changeSet author="phan" id="15">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="container" />
			</not>
		</preConditions>
		<comment>Add column container to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="container" type="integer">
				<constraints foreignKeyName="F_NDER_REL_CONTAINER"
					referencedTableName="NDER_CONTAINER" referencedColumnNames="id_nder_container" nullable="false" />
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="16">
		<preConditions onFail="MARK_RAN">
			<not>
				<indexExists indexName="I_NDER_RELATED_ID_CONT" />
			</not>
		</preConditions>
		<comment>Create index I_NDER_RELATED_ID_CONT if it doesn't exist.</comment>
		<createIndex tableName="NDER_RELATED_ID" indexName="I_NDER_RELATED_ID_CONT">
			<column name="container" />
		</createIndex>
	</changeSet>

</databaseChangeLog>
