		</preConditions>
		<comment>Add column messageid to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="messageid" type="varchar(50)">
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="7">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="containersn" />
			</not>
		</preConditions>
		<comment>Add column containersn to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="containersn" type="varchar(9)">
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="8">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="actionsn" />
			</not>
		</preConditions>
		<comment>Add column actionsn to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="actionsn" type="varchar(9)">
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="9">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="relatedmessage" />
			</not>
		</preConditions>
		<comment>Add column relatedmessage to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="relatedmessage" type="integer">
				<constraints foreignKeyName="F_NDER_REL_REL_MESSAGE"
					referencedTableName="NDER_MESSAGE" referencedColumnNames="id_nder_message" />
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="10">
		<preConditions onFail="MARK_RAN">
			<not>
				<indexExists indexName="I_NDER_RELATED_MESSAGE" />
