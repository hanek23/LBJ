		<createIndex tableName="NDER_RELATED_ID" indexName="I_NDER_RELATED_CONTAINER">
			<column name="relatedcontainer" />
		</createIndex>
	</changeSet>

	<changeSet author="phan" id="13">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="NDER_RELATED_ID" columnName="relatedaction" />
			</not>
		</preConditions>
		<comment>Add column relatedaction to NDER_RELATED_ID</comment>
		<addColumn tableName="NDER_RELATED_ID">
			<column name="relatedaction" type="integer">
				<constraints foreignKeyName="F_NDER_REL_REL_ACTION"
					referencedTableName="NDER_ACTION" referencedColumnNames="id_nder_action" />
			</column>
		</addColumn>
	</changeSet>

	<changeSet author="phan" id="14">
		<preConditions onFail="MARK_RAN">
			<not>
				<indexExists indexName="I_NDER_RELATED_ACTION" />
			</not>
		</preConditions>
		<comment>Create index I_NDER_RELATED_ACTION if it doesn't exist.</comment>
		<createIndex tableName="NDER_RELATED_ID" indexName="I_NDER_RELATED_ACTION">
